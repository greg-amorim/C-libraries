#include <stdio.h>

int    verif(int argc, char *argv[])
{
  if (argc <= 1)
    printf("Aucun arguments ou erreurs\n");
  return (0);

}

int error(int argc, char *argv[])
{
  FILE *fp;

  if ((fp = freopen ("toto.txt", "w", stdout)) == NULL);
      return (0);
      printf("c'est la sortie");
      printf(stdout);
}


int main(int argc, char *argv[])
{

  verif(argc, argv);
  error(argc,argv);
  return (0);


}

